{
  "name": "@danielkalen/subnotices-engine",
  "version": "3.0.0",
  "description": "A simple notice and subnotice display system",
  "main": "dist/subnotices.js",
  "repository": "https://github.com/danielkalen/subnotices-engine",
  "author": "danielkalen",
  "license": "MIT",
  "scripts": {
    "postversion": "echo \\'$(json -f package.json version)\\' > .version.coffee && npm run build",
    "build": "npm run compile && npm run minify",
    "compile": "simplyimport -i src/subnotices.coffee | coffee -c -s -b > dist/subnotices.js",
    "minify": "closure-service dist/subnotices.js > dist/subnotices.min.js",
    "watch": "simplywatch -g 'src/*' -x 'npm run compile -s'"
  },
  "devDependencies": {
    "closure-compiler-service": "^0.5.0",
    "coffee-script": "^1.10.0",
    "simplyimport": "*",
    "simplywatch": "*",
    "uglify": "*"
  },
  "dependencies": {
    "jquery": "^3.1.1"
  }
}
